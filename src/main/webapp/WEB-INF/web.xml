<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>controller.LoginServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>controller.LogoutServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>SignUpServlet</servlet-name>
    <servlet-class>controller.SignUpServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>SignUpServlet</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>AssignVehicle</servlet-name>
    <servlet-class>controller.AssignVehicleServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>AssignVehicle</servlet-name>
    <url-pattern>/assignVehicle</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>controller.RegisterServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>GestisciAbb</servlet-name>
    <servlet-class>controller.GestisciAbbServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>GestisciAbb</servlet-name>
    <url-pattern>/gestisciAbb</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>TelepassPlus</servlet-name>
    <servlet-class>controller.TelepassPlusServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>TelepassPlus</servlet-name>
    <url-pattern>/telepassPlus</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>SimulationServlet</servlet-name>
    <servlet-class>controller.SimulationServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>SimulationServlet</servlet-name>
    <url-pattern>/simulation</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>HandleDeviceServlet</servlet-name>
    <servlet-class>controller.HandleDeviceServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>HandleDeviceServlet</servlet-name>
    <url-pattern>/admin</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>AddTransponderServlet</servlet-name>
    <servlet-class>controller.AddTransponderServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>AddTransponderServlet</servlet-name>
    <url-pattern>/admin/aggiungiTransponder</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>RevokeTransponderServlet</servlet-name>
    <servlet-class>controller.RevokeTransponderServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>RevokeTransponderServlet</servlet-name>
    <url-pattern>/admin/revocaTransponder</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ViewStatisticsServlet</servlet-name>
    <servlet-class>controller.ViewStatisticsServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ViewStatisticsServlet</servlet-name>
    <url-pattern>/admin/visualizzaStatistiche</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>index</servlet-name>
    <jsp-file>/index.jsp</jsp-file>
  </servlet>

  <servlet-mapping>
    <servlet-name>index</servlet-name>
    <url-pattern>/Telepass/</url-pattern>
  </servlet-mapping>

</web-app>
